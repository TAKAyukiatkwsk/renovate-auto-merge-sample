{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "github.com/aws/aws-sdk-go-v2",
        "github.com/aws/aws-sdk-go-v2/config",
        "github.com/aws/aws-sdk-go-v2/service/sts"
      ],
      "matchUpdateTypes": [
        "patch"
      ],
      "automerge": true
    }
  ]
}
